{
  "name": "di-txma-ticf-query-results-delivery",
  "description": "Deliver results of Athena queries to TICF analysts",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphagov/di-txma-ticf-query-results-delivery.git"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "scripts": {
    "allure": "./tests/run-allure.sh",
    "allure:stop": "docker-compose -f ./tests/docker-compose.allure.yaml down",
    "allure:report": "cp ./tests/reports/allure-reports/emailable-report-allure-docker-service.html ./tests/reports/TxMA-Team2-Query-Results-Test-Report-$(date +%d-%m-%Y-%T).html",
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest -c jest.config.ts",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:integration": "jest -c tests/integration-tests/jest.integration.config.ts",
    "test:integration:dev": "jest --setupFiles '<rootDir>/.env' -c tests/integration-tests/jest.integration.config.ts",
    "test:e2e": "jest -c tests/e2e-tests/jest.e2e.config.ts",
    "test:e2e:dev": "jest --setupFiles '<rootDir>/.env' -c tests/e2e-tests/jest.e2e.config.ts"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.5.0",
    "@aws-sdk/client-dynamodb": "3.186.0",
    "@aws-sdk/client-s3": "3.188.0",
    "@aws-sdk/client-secrets-manager": "3.163.0",
    "@aws-sdk/client-sqs": "3.170.0",
    "@aws-sdk/s3-request-presigner": "3.188.0",
    "notifications-node-client": "5.1.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.224.0",
    "@aws-sdk/client-lambda": "3.163.0",
    "@aws-sdk/client-ssm": "3.163.0",
    "@jest/types": "29.1.2",
    "@types/aws-lambda": "8.10.106",
    "@types/jest": "29.1.2",
    "@types/jest-when": "3.5.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "aws-sdk-client-mock": "1.0.0",
    "aws-sdk-client-mock-jest": "2.0.0",
    "axios": "0.27.2",
    "esbuild": "0.15.12",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "husky": "8.0.3",
    "jest": "29.1.2",
    "jest-extended": "3.1.0",
    "jest-junit": "14.0.1",
    "jest-when": "3.5.1",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "13.0.3",
    "node-html-parser": "6.1.1",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yaml-cfn": "0.3.1"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
