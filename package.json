{
  "name": "di-txma-ticf-query-results-delivery",
  "description": "Deliver results of Athena queries to TICF analysts",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphagov/di-txma-ticf-query-results-delivery.git"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest -c jest.config.ts",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:integration": "jest -c tests/integration-tests/jest.integration.config.ts",
    "test:integration:dev": "jest --setupFiles '<rootDir>/.env' -c tests/integration-tests/jest.integration.config.ts",
    "test:e2e": "jest -c tests/e2e-tests/jest.e2e.config.ts",
    "test:e2e:dev": "jest --setupFiles '<rootDir>/.env' -c tests/e2e-tests/jest.e2e.config.ts"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.9.0",
    "@aws-sdk/client-dynamodb": "3.352.0",
    "@aws-sdk/client-s3": "3.352.0",
    "@aws-sdk/client-secrets-manager": "3.352.0",
    "@aws-sdk/client-sqs": "3.352.0",
    "@aws-sdk/s3-request-presigner": "3.352.0",
    "@aws-sdk/util-endpoints": "3.352.0",
    "notifications-node-client": "7.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.352.0",
    "@aws-sdk/client-lambda": "3.352.0",
    "@aws-sdk/client-ssm": "3.352.0",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.2",
    "@types/jest-when": "3.5.2",
    "@types/jsonwebtoken": "9.0.0",
    "@types/node": "18.16.19",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "aws-sdk-client-mock": "2.1.1",
    "aws-sdk-client-mock-jest": "2.1.1",
    "axios": "1.4.0",
    "esbuild": "0.18.3",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.1.3",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-extended": "3.2.4",
    "jest-junit": "14.0.1",
    "jest-when": "3.5.2",
    "jsonwebtoken": "9.0.0",
    "lint-staged": "13.2.2",
    "node-html-parser": "6.1.5",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yaml-cfn": "0.3.2"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
