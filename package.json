{
  "name": "txma-ticf-query-results-delivery",
  "description": "Deliver results of Athena queries to TICF analysts",
  "repository": {
    "type": "git",
    "url": "https://github.com/govuk-one-login/txma-ticf-query-results-delivery.git"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest -c jest.config.ts",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:integration": "jest -c tests/integration-tests/jest.integration.config.ts",
    "test:integration:dev": "jest --setupFiles '<rootDir>/.env' -c tests/integration-tests/jest.integration.config.ts",
    "test:e2e": "jest -c tests/e2e-tests/jest.e2e.config.ts",
    "test:e2e:dev": "jest --setupFiles '<rootDir>/.env' -c tests/e2e-tests/jest.e2e.config.ts"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.14.0",
    "@aws-sdk/client-dynamodb": "3.398.0",
    "@aws-sdk/client-s3": "3.400.0",
    "@aws-sdk/client-secrets-manager": "3.398.0",
    "@aws-sdk/client-sqs": "3.398.0",
    "@aws-sdk/s3-request-presigner": "3.400.0",
    "@aws-sdk/util-endpoints": "3.398.0",
    "notifications-node-client": "7.0.3"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.398.0",
    "@aws-sdk/client-lambda": "3.398.0",
    "@aws-sdk/client-ssm": "3.398.0",
    "@types/aws-lambda": "8.10.125",
    "@types/jest": "29.5.6",
    "@types/jest-when": "3.5.4",
    "@types/jsonwebtoken": "9.0.4",
    "@types/node": "18.18.6",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "aws-sdk-client-mock": "3.0.0",
    "aws-sdk-client-mock-jest": "3.0.0",
    "axios": "1.6.7",
    "esbuild": "0.19.5",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "jest-junit": "16.0.0",
    "jest-when": "3.6.0",
    "jsonwebtoken": "9.0.2",
    "lint-staged": "14.0.1",
    "node-html-parser": "6.1.10",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yaml-cfn": "0.3.2"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
